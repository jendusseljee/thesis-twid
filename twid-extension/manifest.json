{
  "manifest_version": 3,

  "name": "Twid Extension",
  "description": "This extension will show Twid signatures on Twitter accounts.",
  "version": "1.0",
  "action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": ["tabs", "scripting"],
  "host_permissions": ["https://twitter.com/*", "http://localhost:3000/*"]
}
